include ./includes/headUczen.jade
        script(src="/js/uczen/szukaj.js")
    body
    include ./includes/menuUczen.jade
    block content
        div.login-wrap
            div.title-page="Testy"
            input#dataS.input(type="text" placeholder="Nazwa testu")
            spane#buttonS.button="Szukaj"
            hr
            br
            table.tabela1
                thead(style="width:100%;")
                    tr
                        td(style="width:10%;")="Lp."
                        td(style="width:60%;")="Nazwa"
                        td(style="width:30%;")="Opcje"
                tbody#tabelBody
                    for dane , i in data
                        tr
                            td=(i+1)+"."
                            td=dane.name
                            td
                                a.button(style="margin-bottom:5px;" href="/uczen/testy/widok/"+dane.name)="Widok"
                    else
                        tr
                            td=""
                            td(style="color:#ff0000;font-size:50px;")="Nie znaleziono test√≥w do edycji "
                            td=""
    if alert
        div.blankT
        div#alert.alert
            div.title-page="Powiadomienie"
            span=alert
            br
            br
            span#closeA.button="Zamknij"
        script(src="/js/admin/powiadomienie.js")